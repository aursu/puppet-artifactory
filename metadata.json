{
  "name": "aursu-artifactory",
  "version": "0.1.0",
  "author": "aursu",
  "summary": "Artifactory OSS installation and management",
  "license": "Apache-2.0",
  "source": "https://github.com/aursu/puppet-artifactory",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 3.2.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/java",
      "version_requirement": ">= 2.4.0 < 3.0.0"
    },
    {
      "name": "aursu/lsys",
      "version_requirement": ">= 0.1.1 < 1.0.0"
    },
    {
      "name": "aursu/javalocal",
      "version_requirement": ">= 0.1.0 < 1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [
        "25",
        "26",
        "27"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.7.0 < 6.0.0"
    }
  ],
  "pdk-version": "1.7.0",
  "template-url": "https://github.com/puppetlabs/pdk-templates",
  "template-ref": "1.7.0-0-g57412ed"
}
